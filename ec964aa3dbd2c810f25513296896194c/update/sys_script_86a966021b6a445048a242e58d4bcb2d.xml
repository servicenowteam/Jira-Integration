        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
  var gr = new GlideRecord('x_momo_jira_integr_issue');
  if (!current) return;
  var issue = gr.get(current.table_sys_id.toString());
  if (issue) {
    var record = gr;
    if (!record.isNewRecord() && record.getValue('approval') == 'approved') {
      gs.include('JiraAttachment');
      var request = new JiraAttachment();
      request.sendMultipartRequest(record, current);
    }
  }
