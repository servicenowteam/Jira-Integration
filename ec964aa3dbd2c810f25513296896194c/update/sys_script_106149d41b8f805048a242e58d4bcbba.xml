        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
  if (current.getValue('sys_updated_by') == 'guest') return;
  gs.include('JiraRequestV2');
  var request = new JiraRequestV2();
  var assignedTo = current.assigned_to.getRefRecord();
  var accountID = assignedTo.getValue('x_momo_jira_integr_account_id');
  request.setOptions({
    path: 'issue/' + current.getValue('external_id') + '/assignee',
    method: 'PUT',
    body: {
      accountId: accountID
    },
    async: true
  });

  request.send();
})(current, previous);]]></script>
