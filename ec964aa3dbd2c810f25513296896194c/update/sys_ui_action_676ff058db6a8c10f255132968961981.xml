        <name>Import Priorities</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[gs.include('JiraRequestV2');
var util = new global.ArrayUtil();
var request = new JiraRequestV2();

request.send({
  path: 'priority'
}).getResponse(function(results) {
  if (!Array.isArray(results)) return;
  if (results.length === 0) return;
  var gPriorities = new GlideRecord('x_momo_jira_integr_priority');
  var alreadyExistsInTable = [];
  gPriorities.query();
  while (gPriorities.next()) {
    alreadyExistsInTable.push(gPriorities.getValue('external_id'));
  }

  results.filter(function(priority) {
    return !util.contains(alreadyExistsInTable, priority.id);
  }).map(function (item) {
    return {
      external_id: item.id,
      color: item.statusColor,
      name: item.name,
      description: item.description,
      icon: item.iconUrl
    };
  }).forEach(function(priority) {
    var instance = new GlideRecord('x_momo_jira_integr_priority');
    instance.initialize();
    
    var keys = Object.keys(priority);
    for (var i in keys) {
      instance[keys[i]] = priority[keys[i]];
    }
    instance.insert();
  });
});]]></script>
