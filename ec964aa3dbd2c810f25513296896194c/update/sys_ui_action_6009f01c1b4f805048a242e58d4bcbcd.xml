        <name>Import Jira Accounts</name>
        <onclick/>
        <order>200</order>
        <script><![CDATA[gs.include('JiraRequest');

var start = 0;
var isDone = false;
// Honestly, this is just a placeholder
// really the break comes from when we stop
// receiving results
while (!isDone) {
  var users = getUsers(start);
  if (!users) break;
  start += 101;
}


function getUsers(startAt) {
  var request = new JiraRequest();

  var response = request.getAllUsers(startAt);
  if (!response) return false;
  if (Array.isArray(response)) {
    var results = response.filter(function (user) {
      return user.active;
    });
    if (results.length === 0) return false;
    for (var i in results) {
      var result = results[i];
      var gr = new GlideRecord('sys_user');
      gr.addQuery('active', true);
      gr.addQuery('name', result.displayName.trim());
      gr.query();
      while (gr.next()) {
        gr.setValue('x_momo_jira_integr_account_id', result.accountId);
        gr.update();
      }
    }
    return true;
  }
  return false;
}]]></script>
