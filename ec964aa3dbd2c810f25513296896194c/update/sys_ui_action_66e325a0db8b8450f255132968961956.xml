        <name>Import Fields</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[gs.include('JiraRequestV2');

var request = new JiraRequestV2();
request.setOptions({
  path: 'field'
});
request.send();
request.getResponse(createFieldFromJira);

function createFieldFromJira(response) {
  if (Array.isArray(response)) {
    for (var i in response) {
      var table = 'x_momo_jira_integr_jira_fields';
      var gField = new GlideRecord(table);
      if (!gField.get("field_id", response[i].id)) {
        if (response[i].schema) {
          createJiraField(table, response[i]);
        }
      }
    }
  }
}

function createJiraField(table, field) {
  var gr = new GlideRecord(table);
  gr.initialize();
  gr.field_id = field.id;
  gr.field_name = field.name;
  gr.schema_type = field.schema.type;
  
  gr.custom = field.custom;
  if (field.schema.items) {
    gr.schema_items_type = field.schema.items;
  }
  if (field.schema.custom) {
    gr.custom_id = field.schema.customId;
    gr.schema_custom_type = field.schema.custom;
  } else {
    gr.system_label = field.schema.system;
  }
  gr.insert();
  return gr;
}]]></script>
