        <name>Create Issue</name>
        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {
  gs.include("JiraWebhookHelper");
  gs.include("JiraAttachment");

  var helper = new JiraWebhookHelper(request, response);

  var created = helper.createSNIssue();

  if (created) {
    helper.createIssueLinks(created);

    var attachments = helper.getJiraAttachments();

    for (var a = 0; a < attachments.length; a++) {
      var attachment = new JiraAttachment();
      attachment.createAttachmentFromContentUrl(created, attachments[a]);
    }
    helper.setWebhookResponse(201);
  } else {
    helper.setWebhookResponse(400);
  }

})(request, response);]]></operation_script>
