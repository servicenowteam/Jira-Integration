        <script><![CDATA[(function() {
  /* populate the 'data' object */
  /* e.g., data.table = $sp.getValue('table'); */
  data.table = (input ? input.table : undefined) || options.table || $sp.getParameter('table');
  var config = {};
  if (options.chart) {
    config.chart = options.chart;
  }
  var validPlotOptions = hasValidChartOptions('plotOptions', options.plotOptions, config.chart);
  if (options.plotOptions && validPlotOptions) {
    config.plotOptions = options.plotOptions;
  }

  var series = setSeriesArray(data.table, options.seriesData, {
    name: options.chartName,
    colorByPoint: options.chartColorByPoint ? options.chartColorByPoint : false
  });

  config.series = series;

  config.title = options.title ? options.title : 'Pie Chart';

  function hasValidChartOptions(type, opts, chartOptions) {
    if (typeof chartOptions !== 'object' && typeof opts !== 'object') return false;
    if (type === 'plotOptions') {
      var chartType = chartOptions.type;
      if (!opts.hasOwnProperty(chartType)) return false;
      if (typeof opts[chartType] !== 'object') return false;
      return true;
    }
    return false;
  }

  function setSeriesArray(table, query, opts) {
    var _data = [];
    if (!Array.isArray(query)) return [];

    for (var i in query) {
      var collectionItem = Object.extend({}, query[i]);

      collectionItem.y = getAggregateSeriesData(table, 'COUNT', collectionItem.encodedQuery);
      collectionItem.name = collectionItem.name;
      delete collectionItem.encodedQuery;
      _data.push(collectionItem);
    }
    return [{
      name: opts.name,
      colorByPoint: opts.colorByPoint,
      data: _data 
    }];
  }

  function getAggregateSeriesData(table, aggregate, enconded_query) {
    var ga = new GlideAggregate(table);
    ga.addAggregate(aggregate);
    ga.addEncodedQuery(enconded_query);
    ga.query();
    if (ga.next()) return ga.getAggregate(aggregate);
    return 0;
  }
})();]]></script>
