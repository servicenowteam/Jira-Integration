        <description/>
        <name>MarkdownToHTML</name>
        <script><![CDATA[var MarkdownToHTML = Class.create();
MarkdownToHTML.prototype = {
  md: null,
  tokens: [],
  initialize: function(markdown) {
    if (typeof markdown === 'string') {
      this.md = markdown;
    }
  },
  tokenize: function() {
    if (!this.md) return this;

    var lines = this.md.split(/\n/gm);
    
    for (var i in lines) {
      var line = lines[i];
      if (lines.length === (i + 1)) {
        break;
      }
      var nextLine = lines[i + 1];
      if ((line == '' || !line) && (nextLine == '' || !nextLine)) {
        this.tokens.push({
          type: 'line',
          toHTML: '<br />'
        });
      } else if (/^#(.*?)^/gi.test(line)) {
        this.tokens.push({
          type: ''
        });
      }
    }
    return this;
  },
  toHTML: function() {
    if (this.tokens.length === 0) return '';
    return this.tokens.map(function(token) {
        return token.toHTML; 
    }).join('');
  },
  type: 'MarkdownToHTML'
};]]></script>
