<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_script_email">
    <sys_script_email action="DELETE">
        <name>jira_sp_steps</name>
        <new_lines_to_html>false</new_lines_to_html>
        <script><![CDATA[(function runMailScript(current, template, email, email_action, event) {
  var issueType = current.getValue('issue_type');
  var status = current.getValue('status');

  var steps = getRelatedSteps(issueType, status);

  for (var i=0;i<steps.length;i++) {
    var step = steps[i];
    template.print('<td class="step'+ (step.isActive ? " active" : "") +'">');
    template.print('<img src="step-'+ (i + 1) + (step.isActive ? "-active" : '') +'.jpgx" /><br />');
    template.print(step.name);
    template.print('</td>');
  }

  function getRelatedSteps(issueTypeId, currentStatus) {
    var types = [];
    var gr = new GlideRecord('x_momo_jira_integr_status');
    gr.addQuery('issue_type', issueTypeId);
    gr.setLimit(3);
    gr.query();
    while (gr.next()) {
      types.push({
        name: gr.getValue('name'),
        isActive: gr.getUniqueValue() == currentStatus
      });
    }
    return types;
  }
})(current, template, email, email_action, event);]]></script>
