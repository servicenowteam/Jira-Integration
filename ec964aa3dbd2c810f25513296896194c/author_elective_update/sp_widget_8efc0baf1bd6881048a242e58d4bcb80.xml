        <script><![CDATA[(function requestScript() {
  /* populate the 'data' object */
  /* e.g., data.table = $sp.getValue('table'); */
  data.table = (input ? input.table : undefined) || options.table || $sp.getValue('table');
  data.query = (input ? input.query : undefined) || options.query || $sp.getValue('sysparm_query');
  data.orderBy = (input ? input.orderBy : undefined);
  data.limit = options.limit || $sp.getValue('sysparm_limit');
  data.headers = options.headers;
  data.form_name = (input ? input.form : undefined) || options.form || $sp.getValue('form');
  data.requests = [];

  var gr = new GlideRecord('sc_req_item');
  gr.addQuery('active', true);
  gr.addEncodedQuery(data.query);

  if (data.orderBy) {
    gr.orderBy(data.orderBy);
  } else {
    gr.orderBy('number');
  }

  gr.setLimit(data.limit || 100);

  while (gr.next()) {
    data.requests.push({
      number: gr.getDisplayValue('number'),
      priority: gr.getDisplayValue('priority'),
      state: gr.getDisplayValue('state'),
      assigned_to: gr.getDisplayValue('assigned_to'),
      short_description: gr.getValue('short_description'),
      link: '?id=' + data.form_name + '&table=' + data.table + '&sys_id=' + gr.getValue('sys_id')
    });
  }
})();]]></script>
