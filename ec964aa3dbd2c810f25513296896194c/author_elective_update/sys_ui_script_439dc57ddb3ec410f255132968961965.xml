        <name>x_momo_jira_integr.jiraStepper</name>
        <script><![CDATA['use strict';

jiraStepper.$inject = ['$templateCache'];

angular
  .module('jira.stepper', ['ngAnimate', 'ui.bootstrap'])
  .directive('jiraStepper', jiraStepper);

function jiraStepper($templateCache) {
  jiraStepperController.$inject = ['$scope'];
  var directive = {
    restrict: 'E',
    replace: true,
    scope: {
      steps: '=',
      includeStepNumber: '=',
      includeStepTitle: '=',
      cacheTemplates: "="
    },
    template: $templateCache.get('jira.stepper.html'),
    controller: jiraStepperController,
    controllerAs: 'c',
    bindToController: true
  };

  /** @ngInject */
  function jiraStepperController ($scope) {
    $scope.toggleSteps = function(index) {
      $scope.steps[index].expanded = !$scope.steps[index].expanded;

      $scope.steps = c.steps.map(function(step, i) {
        if (step.expanded && i != index) {
          step.expanded = false;
        }
        return step;
      });
    };
  }

  return directive;

  
}

angular
  .module('jira.stepper')
  .run(['$templateCache', function($templateCache) {
    $templateCache.put('jira.stepper.html', '<div class=\"stepper\">\n\t<div class=\"step\" ng-repeat=\"step in c.steps track by $index\">\n\t\t<div class=\"step-heading\" \n\t\t\tng-class=\"{\"active\": !step.expanded}\"\n\t\t\tng-click=\"toggleSteps($index)\">\n\t\t\t<div class=\"circle\" ng-if=\"step.number\">{{ step.number }}</div>\n\t\t\t<div class=\"title\" ng-if=\"step.title\">{{ step.title }}</div>\n\t\t\t<div class=\"line\" ng-hide=\"step.number >= steps.length\"></div>\n\t\t\t<div uib-collapse=\"!step.expanded\">\n\t\t\t\t<div class=\"body\">\n\t\t\t\t\t<div ng-bind-html=\"step.template\">\n\t\t\t\t\t\t<button ng-hide=\"step.number >=steps.length\"\n\t\t\t\t\t\t\tng-click=\"toggleSteps($index + 1)\"\n\t\t\t\t\t\t\tclass=\"btn btn-sm btn-primary btn-bg ripple\"\n\t\t\t\t\t\t\tname=\"next\"\n\t\t\t\t\t\t\ttype=\"button\"> Next </button>\n\t\t\t\t\t\t<button ng-hide=\"step.number <= 1\"\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t\tng-click=\"toggleSteps($index - 1)\"\n\t\t\t\t\t\t\tname=\"previous\"\n\t\t\t\t\t\t\ttype=\"button\"\n\t\t\t\t\t\t\tclass=\"btn btn-sm btn-secondary ripple\"> Previous </button>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</div>\n</div></div>');
  }]);]]></script>
