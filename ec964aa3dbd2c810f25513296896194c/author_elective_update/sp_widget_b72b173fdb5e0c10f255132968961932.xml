        <script><![CDATA[(function() {
  /* populate the 'data' object */
  /* e.g., data.table = $sp.getValue('table'); */
  data.table = options.table || $sp.getValue('table') || 'x_momo_jira_integr_issue';
  data.canCreate = gs.hasRole('x_momo_jira_integr.issue_user');
  data.buttons = options.buttons.map(function(button) {
    return {
      name: button.name,
      link: "?id=jira_issue_new_form&table=" + data.table + '&request_type=' + button.params.request_type
    }
  });
})();]]></script>
