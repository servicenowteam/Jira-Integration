        <client_script><![CDATA[function($window) {
  /* widget controller */
  var c = this;
	
	c.onGoBack = function() {
		if ($window.history) {
			$window.history.back();
		} else {
			$window.location.href = '/sp';
		}
	}
}]]></client_script>
